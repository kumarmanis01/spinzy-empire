{
  "name": "ai-tutor-india",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "rocketCritical": {
    "dependencies": [
      "next",
      "react",
      "react-dom",
      "@tailwindcss/typography",
      "recharts"
    ],
    "devDependencies": [
      "typescript",
      "@types/node",
      "@types/react",
      "@types/react-dom",
      "@typescript-eslint/eslint-plugin",
      "@typescript-eslint/parser",
      "eslint-config-next",
      "tailwindcss",
      "autoprefixer",
      "postcss",
      "eslint",
      "prettier",
      "@netlify/plugin-nextjs"
    ],
    "scripts": [
      "dev",
      "build",
      "start",
      "lint",
      "lint:fix",
      "format",
      "serve",
      "type-check"
    ],
    "warning": " CRITICAL: DO NOT REMOVE OR MODIFY ABOVE DEPENDENCIES - Required for Next.js 15 TypeScript app functionality"
  },
  "scripts": {
    "dev": "next dev",
    "dev:next": "npm run lint:orig && npm run type-check:orig && next dev",
    "dev:fast": "next dev",
    "prepare": "node -e \"if(process.env.NODE_ENV !== 'production') { require('child_process').execSync('npx husky install', {stdio: 'inherit'}) }\"",
    "build": "npx prisma generate && tsc -p tsconfig.build.json && npx tsc-alias -p tsconfig.build.json && node scripts/verify-dist.cjs && next build",
    "build:tsc": "tsc -p tsconfig.build.json && npx tsc-alias -p tsconfig.build.json",
    "build:prod": "npx prisma generate && npm run build:tsc && npm run build:workers && next build",
    "start": "next start",
    "start:next": "next start",
    "export": "next export",
    "lint": "eslint \"app\" \"components\" \"lib\" \"src\"",
    "lint:orig": "eslint . --max-warnings=0",
    "test:eslint-rules": "node ./eslint-rules/tests/run-rule-tests.cjs",
    "lint:fix": "eslint \"app/**\" \"components/**\" \"lib/**\" \"scripts/**\" \"src/**\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:orig": "prettier --write \"src/**/*.{ts,tsx,css,md,json}\"",
    "serve": "next start",
    "type-check": "tsc --noEmit",
    "type-check:orig": "tsc -p tsconfig.json --noEmit",
    "clear-auth": "set CONFIRM=true && node -r ts-node/register scripts/clear-auth-tables.ts",
    "pretest": "npx prisma generate",
    "test": "jest",
    "test:ci": "npm run lint:orig && npm run type-check:orig && npm run test:unit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "health-check": "node scripts/health-check.js",
    "test:integration": "node tests/integration/audit-log.test.cjs",
    "test:integration:evaluator": "node --loader ts-node/esm tests/integration/alert-evaluator.test.ts",
    "test:integration:dry-run": "node tests/integration/alert-evaluator-dry-run.test.cjs",
    "test:integration:all": "npm run test:integration && npm run test:integration:evaluator && npm run test:integration:dry-run",
    "metrics:server": "node -r ts-node/register/transpile-only scripts/metricsServer.ts",
    "render:diagrams": "npx @mermaid-js/mermaid-cli -i docs/diagrams/submit-enqueue.mmd -o docs/diagrams/submit-enqueue.png && npx @mermaid-js/mermaid-cli -i docs/diagrams/worker-claim-execute.mmd -o docs/diagrams/worker-claim-execute.png && npx @mermaid-js/mermaid-cli -i docs/diagrams/reconciler-aggregation.mmd -o docs/diagrams/reconciler-aggregation.png",
    "test:unit": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --runInBand",
    "orchestrator:start": "ts-node -r tsconfig-paths/register worker/orchestrator.ts",
    "seed-db-ai-content-infra": "node -r ts-node/register scripts/seed-database.ts",
    "reset-db-ai-content-infra": "node -r ts-node/register scripts/reset-remote-db.ts",
    "seed-and-reset-db-ai-content-infra": "npm run seed-db-ai-content-infra && npm run reset-db-ai-content-infra",
    "build:workers": "npx prisma generate && tsc -p tsconfig.workers.json && npx tsc-alias -p tsconfig.workers.json && node scripts/fix-dist-imports.cjs dist/worker",
    "postbuild": "node scripts/fix-dist-imports.cjs dist/worker && node scripts/ensure-regeneration-shim.cjs ~\\ai-tutor || true",
    "dist:shim": "node scripts/create-dist-shims.cjs",
    "dist:parse-shim": "node scripts/parse-and-shim.cjs --log logs/worker-error.log --root dist",
    "pm2:start-worker": "pm2 start ecosystem.config.cjs --only content-engine-worker --update-env",
    "pm2:restart-worker": "pm2 restart content-engine-worker --update-env",
    "check-redis-keys": "node scripts/check-redis-keys.js",
    "verify:vps": "bash scripts/vps-verification.sh --auto",
    "verify:dist": "node scripts/verify-dist.cjs",
    "ensure:env": "bash scripts/ensure-env-perms.sh",
    "ci:check-env": "node scripts/check-env-not-tracked.cjs",
    "ci:prebuild": "npm run ci:check-env && npm run verify:dist",
    "deploy:prep": "bash scripts/ensure-env-perms.sh && npm run verify:dist",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "worker:start": "tsx src/workers/index.ts",
    "reconciler:start": "tsx src/jobs/reconciler/index.ts",
    "outbox:start": "tsx src/jobs/outbox/dispatcher.ts",
    "docker:build": "docker build -t edtech-backend .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@aws-sdk/client-s3": "^3.428.0",
    "@aws-sdk/client-secrets-manager": "^3.940.0",
    "@aws-sdk/s3-request-presigner": "^3.428.0",
    "@heroicons/react": "^2.2.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "6.19.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@sentry/nextjs": "^10.36.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@yusifaliyevpro/countries": "^2.2.2",
    "accept-language-parser": "^1.5.0",
    "autoprefixer": "10.4.2",
    "aws-sdk": "^1.18.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.66.0",
    "chalk": "^4.1.2",
    "framer-motion": "^12.23.16",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.544.0",
    "minimist": "^1.2.8",
    "nanoid": "^5.1.6",
    "next": "^16.1.6",
    "next-auth": "^4.24.11",
    "next-pwa": "^5.6.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.11",
    "openai": "^5.23.2",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^2.4.5",
    "pdfjs-dist": "^5.4.449",
    "postcss": "^8.5.6",
    "prom-client": "^15.1.3",
    "razorpay": "^2.9.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.7",
    "react-speech-recognition": "^4.0.1",
    "recharts": "^2.15.2",
    "rehype-sanitize": "^5.0.1",
    "sanscript": "^0.0.2",
    "slugify": "^1.6.6",
    "swr": "^2.2.4",
    "tailwindcss": "3.4.6",
    "tesseract.js": "^4.1.1",
    "uuid": "^13.0.0",
    "yargs": "^18.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@kubernetes/client-node": "^1.4.0",
    "@mermaid-js/mermaid-cli": "^11.12.0",
    "@netlify/plugin-nextjs": "^5.11.1",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.2",
    "@types/node": "^20",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.0",
    "@types/uuid": "^10.0.0",
    "@types/yargs": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "express": "^5.2.1",
    "globals": "^16.4.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jiti": "^2.6.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.5.3",
    "prisma": "6.19.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.44.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "overrides": {
    "cookie": "^0.7.2"
  }
}
